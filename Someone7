local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Someone Hub Beta", "DarkTheme")

-- Set colors
local cyanColor = Color3.fromRGB(0, 255, 255)
local blackColor = Color3.fromRGB(0, 0, 0)

local FarmTab = Window:NewTab("Farm", cyanColor)
local SettingsTab = Window:NewTab("Settings", cyanColor)
local EspTab = Window:NewTab("ESP", cyanColor)

-- Variables
local farmDistance = 5
local flightSpeed = 0
local walkSpeed = 0

-- Bone Farming Section
local BoneFarmSection = FarmTab:NewSection("Auto Farm Bones", blackColor)
local boneFarming = false
local function boneFarm()
    while boneFarming do
        wait(1)
        for _, item in pairs(workspace:GetChildren()) do
            if item:IsA("Part") and item.Name == "Bone" and item:IsTouching(game.Players.LocalPlayer.Character) then
                local player = game.Players.LocalPlayer
                local character = player.Character or player.CharacterAdded:Wait()
                character.Humanoid:MoveTo(item.Position + (item.Position - character.PrimaryPart.Position).unit * farmDistance)
                fireclickdetector(item.ClickDetector)
                wait(0.5)
            end
        end
    end
end

BoneFarmSection:NewToggle("Start/Stop Bone Farming", "Toggles automatic bone farming.", function(state)
    boneFarming = state
    if boneFarming then boneFarm() end
end)

-- Fruit/Raids Section
local FruitRaidsTab = Window:NewTab("Fruit/Raids", cyanColor)
local FruitSection = FruitRaidsTab:NewSection("Auto Collect Fruits", blackColor)
local fruitCollecting = false

local function collectFruits()
    while fruitCollecting do
        wait(1)
        for _, fruit in pairs(workspace:GetChildren()) do
            if fruit:IsA("Fruit") and fruit:IsTouching(game.Players.LocalPlayer.Character) then
                fireclickdetector(fruit.ClickDetector)
                wait(0.5)
            end
        end
    end
end

FruitSection:NewToggle("Start/Stop Fruit Collecting", "Toggles automatic fruit collecting.", function(state)
    fruitCollecting = state
    if fruitCollecting then collectFruits() end
end)

-- Auto Store Fruits Section
local StoreFruitSection = FruitRaidsTab:NewSection("Auto Store Fruits", blackColor)
local fruitStoring = false

local function storeFruits()
    while fruitStoring do
        wait(1)
        local player = game.Players.LocalPlayer
        local inventory = player.Backpack:GetChildren()
        for _, fruit in ipairs(inventory) do
            if fruit:IsA("Fruit") then
                fireclickdetector(fruit.ClickDetector)
                wait(0.5)
            end
        end
    end
end

StoreFruitSection:NewToggle("Start/Stop Fruit Storing", "Toggles automatic fruit storing.", function(state)
    fruitStoring = state
    if fruitStoring then storeFruits() end
end)

-- Level Farming Section
local LevelFarmSection = FarmTab:NewSection("Auto Farm Levels", blackColor)
local autoFarming = false

local function farmLevels()
    while autoFarming do
        wait(1)
        local player = game.Players.LocalPlayer
        local character = player.Character or player.CharacterAdded:Wait()

        for _, enemy in pairs(workspace:GetChildren()) do
            if enemy:IsA("Model") and enemy:FindFirstChild("Humanoid") then
                local humanoid = enemy.Humanoid
                if humanoid.Health > 0 then
                    character:MoveTo(enemy.PrimaryPart.Position + (enemy.PrimaryPart.Position - character.PrimaryPart.Position).unit * farmDistance)
                    humanoid:TakeDamage(10) -- Adjust damage as needed
                    wait(0.5)
                end
            end
        end
    end
end

LevelFarmSection:NewToggle("Start/Stop Auto Level Farming", "Automatically farms levels.", function(state)
    autoFarming = state
    if autoFarming then
        farmLevels()
    end
end)

-- Boss Tracker Section
local BossSection = FarmTab:NewSection("Active Bosses", blackColor)
local bosses = {
    "Gorilla King",
    "Bobby",
    "The Saw",
    "Warden",
    "Chief Warden",
    "Swan",
    "Magma Admiral",
    "Grey Beard",
    "Vice Admiral",
    "Fishman Lord",
    "Wysper",
    "Thunder God",
    "Cyborg"
}

local function updateBossList()
    BossSection:Clear()
    for _, bossName in ipairs(bosses) do
        local boss = workspace:FindFirstChild(bossName)
        BossSection:NewLabel(boss and boss:IsA("Model") and boss:FindFirstChild("Humanoid") and boss.Humanoid.Health > 0 and (bossName .. " is alive!") or (bossName .. " is not alive."))
    end
end

game:GetService("RunService").Stepped:Connect(updateBossList)
updateBossList()

-- Teleport Section
local TeleportSection = FarmTab:NewSection("Teleport to the Seas", blackColor)

local function teleportTo(position)
    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(position)
end

TeleportSection:NewButton("Teleport to Sea 1", "Teleports to Sea 1.", function() teleportTo(Vector3.new(0, 10, 0)) end)
TeleportSection:NewButton("Teleport to Sea 2", "Teleports to Sea 2.", function() teleportTo(Vector3.new(3500, 10, 2000)) end)
TeleportSection:NewButton("Teleport to Sea 3", "Teleports to Sea 3.", function() teleportTo(Vector3.new(12000, 10, 3000)) end)

-- Settings Section
local SettingsSection = SettingsTab:NewSection("Configurations", blackColor)

-- Slider for flight speed
SettingsSection:NewSlider("Flight Speed", "Adjust the player's flight speed.", 500, 0, function(value)
    flightSpeed = value
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    if character and character:FindFirstChild("Humanoid") then
        character.Humanoid.WalkSpeed = flightSpeed
    end
end)

-- Slider for farming distance
SettingsSection:NewSlider("Farming Distance", "Adjust the farming distance from the NPC.", 30, 0, function(value)
    farmDistance = value
    print("Farming distance set to: " .. farmDistance)
end)

-- Slider for walk speed
SettingsSection:NewSlider("Walk Speed", "Adjust the player's walk speed (0 to 240).", 240, 0, function(value)
    walkSpeed = value
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()
    if character and character:FindFirstChild("Humanoid") then
        character.Humanoid.WalkSpeed = walkSpeed
    end
end)

-- Toggle for notifications
SettingsSection:NewToggle("Enable Notifications", "Enables/disables notifications in the game.", function(state)
    if state then
        print("Notifications enabled.")
    else
        print("Notifications disabled.")
    end
end)

-- ESP Section
local EspSection = EspTab:NewSection("Enable ESP", blackColor)

local espEnabled = false
local function createESP(part)
    local esp = Instance.new("BoxHandleAdornment")
    esp.Size = part.Size + Vector3.new(0.1, 0.1, 0.1)
    esp.Adornee = part
    esp.Color3 = cyanColor -- Cyan color
    esp.AlwaysOnTop = true
    esp.ZIndex = 10
    esp.Transparency = 0.5
    esp.Parent = part
end

EspSection:NewToggle("Enable/Disable ESP", "Enables or disables ESP for players and NPCs.", function(state)
    espEnabled = state
    for _, child in pairs(workspace:GetChildren()) do
        if child:IsA("Model") and (child:FindFirstChild("Humanoid") or child:IsA("Part")) then
            if espEnabled then
                createESP(child)
            else
                for _, adornment in pairs(child:GetChildren()) do
                    if adornment:IsA("BoxHandleAdornment") then
                        adornment:Destroy()
                    end
                end
            end
        end
    end
end)

-- Aimbot Section
local AimbotSection = EspTab:NewSection("Aimbot", blackColor)
local aimbotEnabled = false

local function getClosestEnemy()
    local closestEnemy = nil
    local closestDistance = math.huge
    local player = game.Players.LocalPlayer
    local character = player.Character or player.CharacterAdded:Wait()

    for _, enemy in pairs(workspace:GetChildren()) do
        if enemy:IsA("Model") and enemy:FindFirstChild("Humanoid") and enemy.Humanoid.Health > 0 then
            local distance = (character.PrimaryPart.Position - enemy.PrimaryPart.Position).magnitude
